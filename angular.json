{
  "version": 1,
  "projects": {
    "nx-docker": {
      "root": "packages/nx-docker",
      "sourceRoot": "packages/nx-docker/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["packages/nx-docker/tsconfig.lib.json", "packages/nx-docker/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!packages/nx-docker/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/nx-docker/jest.config.js",
            "tsConfig": "packages/nx-docker/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/nx-docker",
            "tsConfig": "packages/nx-docker/tsconfig.lib.json",
            "packageJson": "packages/nx-docker/package.json",
            "main": "packages/nx-docker/src/index.ts",
            "assets": [
              "packages/nx-docker/*.md",
              {
                "input": "./packages/nx-docker/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-docker",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/nx-docker",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "nx-docker-e2e": {
      "projectType": "application",
      "root": "packages/nx-docker-e2e",
      "sourceRoot": "packages/nx-docker-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-docker:build",
            "npmPackageName": "@nx-tools/nx-docker",
            "pluginOutputPath": "dist/packages/nx-docker",
            "jestConfig": "packages/nx-docker-e2e/jest.config.js",
            "tsSpecConfig": "packages/nx-docker-e2e/tsconfig.spec.json"
          }
        }
      }
    },
    "example-app": {
      "root": "packages/example-app",
      "sourceRoot": "packages/example-app/src",
      "projectType": "application",
      "prefix": "example-app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/packages/example-app",
            "main": "packages/example-app/src/main.ts",
            "tsConfig": "packages/example-app/tsconfig.app.json",
            "assets": ["packages/example-app/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "packages/example-app/src/environments/environment.ts",
                  "with": "packages/example-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "example-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["packages/example-app/tsconfig.app.json", "packages/example-app/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!packages/example-app/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/example-app/jest.config.js",
            "tsConfig": "packages/example-app/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "docker": {
          "builder": "./dist/packages/nx-docker:build",
          "options": {
            "repository": "gperdomor/example-app",
            "tagWithRef": true,
            "tagWithSha": true,
            "push": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    }
  }
}
