      "default": "./dist/lib/utils/git.js"
    },
    "./src/lib/utils/github": {
      "development": "./src/lib/utils/github.js",
      "types": "./dist/lib/utils/github.d.ts",
      "import": "./dist/lib/utils/github.js",
      "default": "./dist/lib/utils/github.js"
    }
  }
