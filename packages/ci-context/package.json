  "repository": "https://github.com/gperdomor/nx-tools",
  "bugs": "https://github.com/gperdomor/nx-tools/issues",
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "dependencies": {
    "@nx-tools/core": "6.1.1",
    "ci-info": "^4.0.0",
    "@octokit/openapi-types": "^22.0.0",
    "@actions/github": "^6.0.0",
    "properties-file": "^3.5.4"
  },
  "peerDependencies": {
    "tslib": "^2.5.0"
  },
  "files": [
