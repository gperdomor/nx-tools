{
  "name": "@nx-tools/nx-set-shas",
  "version": "7.2.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/gperdomor/nx-tools",
    "directory": "packages/nx-set-shas"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "pnpm exec oclif manifest && pnpm exec oclif readme"
  },
  "bin": {
    "nx-set-shas": "./bin/run.js"
  },
  "engines": {
    "node": ">=20.19.0 <21 || >=22.12.0 <23 || >=24.0.0"
  },
  "keywords": [
    "nx-set-shas",
    "nx-tools",
    "oclif"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@nx-tools/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@nx-tools/core": "workspace:*",
    "@oclif/core": "4.5.4",
    "@oclif/plugin-help": "6.2.37",
    "semver": "7.7.3"
  },
  "peerDependencies": {
    "tslib": "^2.6.0"
  },
  "oclif": {
    "bin": "nx-set-shas",
    "dirname": "nx-set-shas",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./dist/lib/hooks/node-version"
    },
    "topicSeparator": " ",
    "topics": {}
  }
}
